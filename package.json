{
  "name": "chat-rn",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "12"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 8 Plus'",
    "start": "react-native start --reset-cache",
    "pod:install": "(cd ios && pod install)",
    "pod:update": "(cd ios && pod repo update)",
    "android:build": "(cd android && ./gradlew assembleRelease)",
    "android:upload": "adb install -r ./android/app/build/outputs/apk/release/app-release.apk",
    "test": "jest",
    "lint": "eslint .",
    "gf:serve": "npm run serve --prefix functions",
    "gf:shell": "npm run shell --prefix functions",
    "gf:start": "npm run start --prefix functions",
    "gf:deploy": "npm run deploy --prefix functions",
    "gf:logs": "npm run logs --prefix functions"
  },
